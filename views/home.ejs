<style>
  .glass-effect {
    background: linear-gradient(135deg, #030303, #080808);
    backdrop-filter: blur(10px);
    border-radius: 12px;
    padding: 1rem;
    box-shadow: 0 6px 6px rgba(0, 0, 0, 0.15);
    border: 1px solid #0f0f0f;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    cursor: pointer;
  }
  
  .glass-effect:hover {
    transform: scale(1.05);
    box-shadow: 0 0 20px 8px rgba(255, 255, 255, 0.05);
  }

  .card-container {
    position: relative;
    width: 100%;
    padding-bottom: 100%;
  }
  
  .card-container img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 50%;
  }

  .karyawan-title {
    font-size: 1.875rem;
    font-weight: 300;
    text-align: center;
    color: white;
    margin-bottom: 3rem;
  }

  .karyawan-nama {
    margin-top: 25px;
    font-size: 25px;
    margin-bottom: 10px;
    font-weight: 300;
    text-align: center;
    color: white;
  }

  .karyawan-jabatan {
    font-size: 17px;
    font-weight: 200;
    text-align: center;
    color: #a3a3a3;
    margin-bottom: 3rem;
  }

  .container {
    padding-left: 1rem;
    padding-right: 1rem;
    max-width: 1400px; /* Diperlebar untuk menampung 7 kolom */
    margin: 0 auto;
  }
</style>

<section class="container">
  <div>
    <!-- Data Karyawan -->
    <h2 class="section-title text-center mb-12 text-3xl font-bold text-white">Tim Kami</h2>
    <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 xl:grid-cols-7 gap-4">
      <% karyawan.forEach(karyawan => { %>
        <div class="glass-effect text-center" onclick="openModal('<%= karyawan.id %>')">
          <div class="card-container w-full h-0 pb-100% relative">
            <img 
              src="<% if (karyawan.foto) { %>data:image/jpeg;base64,<%= karyawan.foto.toString('base64') %><% } else { %>default-image.jpg<% } %>" 
              alt="Foto Karyawan" 
              class="w-full h-full object-cover rounded-full mx-auto mb-4 absolute top-0 left-0"
            />
          </div>
          <h2 class="karyawan-nama"><%= karyawan.nama %></h2>
          <p class="karyawan-jabatan"><%= karyawan.jabatan || 'Tidak ada' %></p>
        </div>
      <% }); %>
    </div>
  </div>
</section>



