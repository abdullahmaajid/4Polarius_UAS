  color: #fff; /* Warna teks judul saat hover */
}

.card:hover p {
  color: #ccc; /* Warna teks deskripsi saat hover */
}

.card:hover img {
  filter: brightness(1.2); /* Meningkatkan kecerahan ikon saat hover */
}

/* Animasi tambahan untuk efek modern */
@keyframes float {
  0%, 100% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(-10px);
  }
}

.card:hover {
  animation: float 3s ease-in-out infinite; /* Efek mengambang saat hover */
}
</style>
<!-- Services Section -->
